@using System.Runtime.Remoting.Activation
@using Brewgr.Web
@using Brewgr.Web.Core.Model

@{
	ViewBag.Title = "Send-To-Shop";
}

@section Styles
{
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/sendtoshop.css")" />
}

@section Scripts
{
    <script type="text/javascript" src="@Url.Content("~/js/sendtoshop.js")"></script>
}

<div id="view-sendtoshopconf" class="content"> 
    
    <h2>Your Send-To-Shop Order has been Sent</h2>
    
    <p>
        Thank you for using Brewgr's Send-To-Shop service.  We have sent your selections to @ViewBag.Partner.Name.  You will be receving a confirmation email shortly along with
        additional email messages as your order is processed.
    </p>
    
    <p>
        Happy Brewing!<br />
        The Brewgr Team
    </p>
</div>
